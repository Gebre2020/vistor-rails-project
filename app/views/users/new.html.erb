<h1>Users#new</h1>

<h1>Sign Up</h1>

<%= form_with model: @user do |f| %>

  <%= f.label :username %>
  <%= f.text_field :username %><br><br>

  <%= f.label :email %>
  <%= f.email_field :email, required: true %><br><br>

  <%= f.label :password %>
  <%= f.password_field :password %><br><br>

  <%= f.submit "Create Account" %>

<% end %>